{
  "name": "@jet-ui/react",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Yony Calsin",
  "main": "dist/index.js",
  "module": "dist/jet-ui.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build --name jet-ui",
    "lint": "tsdx lint",
    "prepare": "tsdx build --name jet-ui",
    "size": "size-limit",
    "start": "tsdx watch --name jet-ui",
    "test": "tsdx test --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@size-limit/preset-small-lib": "^6.0.3",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "babel-loader": "^8.2.2",
    "husky": "^7.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "size-limit": "^6.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "size-limit": [
    {
      "path": "dist/jet-ui.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/jet-ui.esm.js",
      "limit": "10 KB"
    }
  ]
}
