{
  "name": "@jet-ui/react",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Yony Calsin",
  "main": "dist/index.js",
  "module": "dist/jet-ui.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/jet-ui.cjs.production.min.js",
      "default": "./dist/jet-ui.esm.js"
    }
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build --name jet-ui",
    "lint": "tsdx lint",
    "prepare": "tsdx build --name jet-ui",
    "size": "size-limit",
    "start": "tsdx watch --name jet-ui",
    "test": "tsdx test --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^6.0.3",
    "husky": "^7.0.2",
    "tsdx": "^0.14.1",
    "react": "^17.0.1"
  },
  "peerDependencies": {
    "react": ">=16.8.6"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "size-limit": [
    {
      "path": "dist/jet-ui.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/jet-ui.esm.js",
      "limit": "10 KB"
    }
  ]
}
